// Syntax:  [ Dig =  Ch, Dur, Del, DDur, DDel ]
// Syntax: Segment/Blank..  :	Dac =  Ch, Dur, A/R,Del ;  Amp	= 	Amp, Scl, DAmp, DDur	

// trakt stimulation, recording of postsynaptic conductance, VC 

PULSE	
Protocol:	Name	= "gamma.oscillation"
Adc:		Chan = 0;		SmpInt = 200; Gain =500; RGB=(50000,0,0); Units =mV
Adc:		Chan = 1;		SmpInt = 200; Gain = 500; RGB=(50000,0,0); Units =mV
Dac:		Chan = 0;		SmpInt = 200; Gain = 0.5; RGB=(0,50000,0); Units =mV// postsynaptic cell


//recording of oscillatory activity in current clamp

Frames:	N	= 100
  Sweeps:	N	= 1         
     Segment:	Dac	= 0, 3000;   Amp	=    0
     Segment:	Dac	= 0, 20;     Amp = 0;                 Dig = 1, 1.5, -0.1
     Segment:	Dac  = 0, 50;	   Amp	=    0;            Dig = 0, 1.5, -0.1  
     Segment:	Dac  = 0, 50;	   Amp	=    0;            Dig = 0, 1.5, -0.1
     Blank:     Dac = 0, 27000; Amp = 0
  EndSweep
EndFrame 

//V-clamp: 100 mV/V, das ergibt bei einer gain = 10 und Amplitude von 10, einen Spannungssprung von 10 mV.